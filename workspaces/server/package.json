{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts",
    "start": "node dist/index.js",
    "prisma:migrate": "npx prisma migrate dev --schema=./src/database/schema/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./src/database/schema/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./src/database/schema/schema.prisma"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/morgan": "1.9.3",
    "prisma": "3.14.0",
    "ts-node-dev": "1.1.8"
  },
  "dependencies": {
    "@prisma/client": "3.14.0",
    "axios": "0.27.2",
    "cheerio": "1.0.0-rc.11",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "morgan": "1.10.0",
    "redis": "4.1.0"
  }
}
